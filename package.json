      "cordova-plugin-mediapicker-dmcsdk": {
        "IOS_PHOTO_LIBRARY_USAGE_DESCRIPTION": "LIN"
      },
      "cordova-plugin-android-fingerprint-auth": {},
      "cordova-plugin-vibration": {},
      "cordova-clipboard": {},
      "cordova-sqlite-storage": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
